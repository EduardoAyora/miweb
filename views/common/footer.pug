mixin footer()
  footer.page-footer
    .container.text-center
      p.blockquote Dando clic tambi√©n puedes encontrarme en:
      a(href="/").text-light
        i.fab.fa-instagram.fa-lg  Instagram
      a(href="/").text-light
        i.fab.fa-youtube.fa-lg  YouTube
  script(src="/javascripts/jquery.min.js")
  script(src="/bootstrap/js/bootstrap.min.js")
  
  script.
    var url = $(location).attr('pathname');
    if(url.includes('/blog')){
      $("#link-blog").addClass("active")
    }
    else if (url.includes('link-portafolio')) {
      $("#link-portafolio").addClass("active")
    }else {
      $("#link-inicio").addClass("active")
    }
